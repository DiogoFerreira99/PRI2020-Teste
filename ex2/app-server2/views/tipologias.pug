extends layout

block content
  h1 Lista de Classes
  .w3-container.w3-margin
    table.w3-table-all
        tr
            th Codigo
            th Titulo

        each t in lista
            tr
                td
                    a(href=`/classes/${t.codigo}`) #{t.codigo}
                td= t.titulo

  h5 
        a(href=`/`) Voltar á página inicial
                
